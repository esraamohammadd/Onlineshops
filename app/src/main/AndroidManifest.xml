<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.online_shop">
<uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon3"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon3"
        android:supportsRtl="true"
        android:theme="@style/Theme.Online_shop"
        tools:targetApi="31">
        <activity
            android:name="com.example.ui.Chat"
            android:exported="false" />
        <activity
            android:name="com.example.ui.Seller"
            android:exported="false" />
        <activity
            android:name="com.example.ui.Customer"
            android:exported="false" />
        <activity
            android:name="com.example.ui.sginUp"
            android:exported="false" />
        <activity
            android:name="com.example.ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>